{
  "name": "EmbeddedBroker",
  "defaultVirtualHost": "default",
  "modelVersion": "1.3",
  "storeVersion": 1,
  "authenticationproviders": [
    {
      "name": "passwordFile",
      "path": "${qpid.pass_file}",
      "type": "PlainPasswordFile"
    }
  ],
  "ports": [
    {
      "name": "AMQP",
      "port": "${qpid.amqp_port}",
      "authenticationProvider": "passwordFile"
    }
  ],
  "virtualhosts": [
    {
      "name": "sportsdata/",
      "storeType": "Memory",
      "type":"STANDARD"
    },
    {
      "name": "sportsdata-consolidation/",
      "storeType": "Memory",
      "type":"STANDARD"
    },
    {
      "name": "opta-messages/",
      "storeType": "Memory",
      "type":"STANDARD"
    }
  ]
}